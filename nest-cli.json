{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "entryFile": "infra/main",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {"include": "../generated/**/*", "outDir": "./dist/generated", "watchAssets": true},
      {"include": "../prisma/**/*", "outDir": "./dist/prisma", "watchAssets": true}
    ]
  }
}
